; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CVR-Y"
config/version="0.0.1_DEV"
run/main_scene="res://Frontend/Startup/Startup.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Frontend/CVRYicon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Frontend/CVRYicon.png"

[autoload]

SaveCrypto="*res://Backend/savecrypto.gd"
FrontStart="*res://Frontend/FrontStart.gd"
Utils="*res://Backend/utils.gd"
ApiCvrHttp="*res://Backend/api_cvr_http.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/HTTPManager/plugin.cfg", "res://addons/discord-rpc-gd/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
