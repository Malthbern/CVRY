[editor_plugins]

enabled=PackedStringArray("res://addons/HTTPManager/plugin.cfg", "res://addons/discord-rpc-gd/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
